<Page x:Class="WpfApplication1.Result"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApplication1"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Result" Width="500" Height="300"
      Loaded="Page_Loaded">

    <Grid Margin="0,0,0,0" Height="300">
        <Button x:Name="buttonFinish" Content="おしまい" Margin="400,263,10,10" FontFamily="KFhimaji" FontSize="14" Click="buttonFinish_Click"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" VerticalAlignment="Top" Height="280" Width="385" Margin="10,10,0,0" DataContext="{Binding}" ItemsSource="{Binding}" FontFamily="KFhimaji" FontSize="14" />
    </Grid>
</Page>
